#makefile#written by richard gordon#used and abused by M. H. Atkinson.KEEP_STATE:EXECUTABLE = testOBJECTS = error.o unsafearray.o safearray.o autoarray.o\	thing.o eqthing.o complex.o compthing.o character.o\	bunch.o test.o$(EXECUTABLE): $(OBJECTS)	g++ $(OBJECTS) -o $(EXECUTABLE).cc.o:	g++ -g -c $< -o $@clean:	rm -f $(EXECUTABLE) $(OBJECTS)	rm -f *~	rm -f #*#	rm -f #*.*#run::	$(EXECUTABLE)